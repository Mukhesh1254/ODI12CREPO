 <Field name="Txt" type="java.lang.String"><![CDATA[SELECT
    -- 
     invoice_number
        || '~' || operating_unit
        || '~' || cust_po_number
        || '~' || batch_source_name
        || '~' || transaction_type_name
        || '~' || transaction_dt_wid as invoice_id,
      invoice_number
        || '~' || invoice_line
        || '~' || operating_unit
        || '~' || cust_po_number
        || '~' || batch_source_name
        || '~' || transaction_type_name
        || '~' || transaction_dt_wid AS integration_id,
      invoice_number
        || '~' || invoice_line as invoice_line_name,  
    --
    datasource_name AS datasource_name,
    customer_account_number AS customer_account_number,
    invoice_number AS invoice_number,
    invoice_line AS invoice_line,
    invoice_currency_code AS invoice_currency_code,
    transaction_date AS transaction_date,
    MAX(gl_date) AS gl_date,
    bill_to_site_use_id AS bill_to_site_use_id,
    ship_to_site_use_id AS ship_to_site_use_id,
    sales_order_number AS sales_order_number,
    transaction_type_name AS transaction_type_name,
    sales_order_line_id AS sales_order_line_id,
    product_number AS product_number,
    product_description AS product_description,
    line_description AS line_description,
    sales_order_line_type_code AS sales_order_line_type_code,
    quantity AS quantity,
    unit_selling_price AS unit_selling_price,
    SUM(revenue_entered_amt) AS revenue_entered_amt,
    SUM(revenue_local_amt) AS revenue_local_amt,
    local_currency_code AS local_currency_code,
    SUM(revenue_usd_amt) AS revenue_usd_amt,
    tax_entered_amt AS tax_entered_amt,
    freight_entered_amt AS freight_entered_amt,
    transaction_entered_amt AS transaction_entered_amt,
    arrs_interface_line_context AS arrs_interface_line_context,
    cust_po_number AS cust_po_number,
    type_code AS type_code,
    operating_unit as operating_unit
FROM (
/* Formatted on 8/10/2016 12:59:20 PM (QP5 v5.139.911.3011) */
SELECT
    ds.source_name datasource_name,
    ou.org_name operating_unit,
    revn.transaction_dt_wid,
    revn.integration_id,
    cust.account_num AS customer_account_number,
    revn.invoice_num AS invoice_number,
    revn.invoice_item AS invoice_line,
    revn.doc_curr_code AS invoice_currency_code,
    dtrx.day_dt AS transaction_date,
    dgl.day_dt AS gl_date,
   -- revn.w_update_dt,
    bill.site_use_id AS bill_to_site_use_id,
    ship.site_use_id AS ship_to_site_use_id,
    revn.x_arrs_interface_ln_attr1 AS sales_order_number,
    xtrx.xact_subtype_name AS transaction_type_name,
    DECODE(revn.x_arrs_interface_line_context,'OKS CONTRACTS',NULL, DECODE(revn.x_arrs_interface_ln_attr1,NULL,NULL,revn.x_arrs_interface_line_attr6)) AS sales_order_line_id,
    prod.part_num AS product_number,
    prod.prod_name AS product_description,
    revn.x_mot_desc AS line_description,
    DECODE(revn.x_arrs_interface_line_context,'OKS CONTRACTS',NULL,xord.xact_subtype_code) AS sales_order_line_type_code,
    --
    SUM (
        CASE
            WHEN revn.datasource_num_id IN (51, 52)
            THEN NVL (revn.xact_qty, 0)
            ELSE
                CASE
                    WHEN glacc.account_seg3_code <> '420000'
                     AND glacc.recon_type_code = 'R'
                     AND revn.x_mot_account_class = 'XLA MANUAL'
                    THEN NVL (revn.xact_qty, 0) * -1
                    ELSE NVL (revn.xact_qty, 0)
                END
        END
    ) AS quantity,
    --
    revn.x_mot_unit_selling_price AS unit_selling_price,
    --
    SUM(
        CASE
            WHEN revn.datasource_num_id IN (51, 52)
            THEN NVL (revn.x_mot_rev_ent_amt, 0)
            ELSE revn.x_mot_rev_ent_amt
        END
    ) AS revenue_entered_amt,
    --
    SUM(
        CASE
            WHEN revn.x_mot_account_class IN ('ADJ', 'ADJUST')
            THEN NVL(revn.revenue_loc_amt,0)
            ELSE NVL(revn.x_mot_rev_ext_amt,0)
        END
    ) AS revenue_local_amt,
    --
    sob.currency_code AS local_currency_code,
    --
    SUM(
        CASE
            WHEN revn.x_mot_account_class IN ('ADJ', 'ADJUST')
            THEN NVL(revn.revenue_loc_amt,0) * OBAV7_R11_DW.wc_common_pkg.exchange_rate(dgl.day_dt,sob.currency_code)
            ELSE NVL(revn.x_mot_rev_ext_amt,0) * OBAV7_R11_DW.wc_common_pkg.exchange_rate(dgl.day_dt,sob.currency_code)
        END
    ) AS revenue_usd_amt,
    --
    SUM (NVL(revn.x_mot_tax_amt,0)) AS tax_entered_amt,
    SUM (NVL(revn.x_mot_freight_amt,0)) AS freight_entered_amt,
    SUM (NVL(revn.x_arrs_total_trx_amt,0)) AS transaction_entered_amt,
    revn.x_arrs_interface_line_context AS arrs_interface_line_context,
    revn.x_mot_cust_po_number AS cust_po_number,
    xtrx.xact_type_code AS type_code,
    revn.x_mot_batch_source_name AS batch_source_name
    --
FROM
    OBAV7_R11_DW.w_customer_loc_use_d bill,
    OBAV7_R11_DW.w_customer_loc_use_d ship, 
    OBAV7_R11_DW.w_day_d dtrx,
    OBAV7_R11_DW.w_gl_account_d glacc,
    OBAV7_R11_DW.w_gl_revn_f revn,
    OBAV7_R11_DW.w_xact_type_d xord,
    OBAV7_R11_DW.w_xact_type_d xtrx,
    OBAV7_R11_DW.w_day_d dgl,
    OBAV7_R11_DW.w_product_d prod,
    OBAV7_R11_DW.w_customer_account_d cust,
    OBAV7_R11_DW.w_party_d party,
    OBAV7_R11_DW.wc_datasource_d ds,
    OBAV7_R11_DW.w_ledger_d sob,
    OBAV7_R11_DW.w_int_org_d ou
WHERE 1 = 1
    AND xtrx.xact_type_code IN ('INV','CM')
    AND xtrx.row_wid = revn.x_mot_trx_type_wid
    AND dtrx.row_wid = revn.transaction_dt_wid
    AND (CASE
            WHEN revn.x_arrs_interface_line_context = 'PS CONTRACTS'
            THEN '400'
            ELSE glacc.account_seg5_code
         END = '400')
    AND glacc.row_wid = revn.x_mot_xla_sec_gl_account_wid
    AND revn.product_wid = prod.row_wid
    AND cust.row_wid = revn.customer_accnt_wid
    AND party.datasource_num_id = cust.datasource_num_id
    AND party.integration_id = cust.party_id
    AND revn.delete_flg = 'N'
    AND revn.x_mot_account_class = 'REV'
    AND revn.sales_org_wid = ou.row_wid
    AND revn.x_arrs_bill_to_site_use_wid = bill.row_wid
    AND revn.x_arrs_ship_to_site_use_wid = ship.row_wid
    AND revn.posted_on_dt_wid = dgl.row_wid
    AND revn.x_mot_order_line_type_wid = xord.row_wid
    AND (xord.row_wid IN (0) OR xord.xact_code IN ('SALES_ORDLNS'))
    AND revn.datasource_num_id = ds.datasource_num_id
    AND ds.source_name = 'OMSC'
    AND revn.ledger_wid = sob.row_wid
    -- ------------------------------------------------------------------------
    -- Change the line below to query based on different transaction dates.
    -- ------------------------------------------------------------------------
    AND dtrx.day_dt >=  TO_TIMESTAMP ('#INFA2PAAS.LV_INVOICE_GET_LOADTIME', 'YYYY-MM-DD HH24:MI:SS.FF1')
    AND revn.invoice_num IN 
    ( select revn1.invoice_num from OBAV7_R11_DW.w_gl_revn_f revn1, OBAV7_R11_DW.w_day_d dgl1
      WHERE 1=1
      AND revn1.posted_on_dt_wid = dgl1.row_wid
       -- ------------------------------------------------------------------------
       -- Change the line below to query based on different GL dates.
       -- ------------------------------------------------------------------------
      AND dgl1.day_dt >=  TO_TIMESTAMP ('#INFA2PAAS.LV_INVOICE_GET_LOADTIME', 'YYYY-MM-DD HH24:MI:SS.FF1') 
    )
    -- ------------------------------------------------------------------------
   GROUP BY
    ds.source_name,
    ou.org_name,
    revn.transaction_dt_wid,
    revn.integration_id,
    cust.account_num,
    xtrx.xact_subtype_name,
    dtrx.day_dt,
    revn.doc_curr_code,
    revn.x_arrs_interface_ln_attr1,
    revn.invoice_num,
    revn.invoice_item,
    revn.x_mot_unit_selling_price,
    revn.x_mot_desc,
    DECODE(revn.x_arrs_interface_line_context,'OKS CONTRACTS',NULL, DECODE(revn.x_arrs_interface_ln_attr1,NULL,NULL,revn.x_arrs_interface_line_attr6)),
    xord.xact_subtype_code,
    dgl.day_dt,
    prod.prod_name,
    prod.part_num,
    bill.site_use_id,
    ship.site_use_id,
    revn.x_arrs_interface_line_context,
    sob.currency_code,
    revn.x_mot_cust_po_number,
    xtrx.xact_type_code,
    revn.x_mot_batch_source_name
)
GROUP BY
    operating_unit,
    transaction_dt_wid,
    invoice_number,
    invoice_line,
    datasource_name,
    customer_account_number,
    invoice_number,
    invoice_line,
    invoice_currency_code,
    transaction_date,
    bill_to_site_use_id,
    ship_to_site_use_id,
    sales_order_number,
    transaction_type_name,
    sales_order_line_id,
    product_number,
    product_description,
    line_description,
    sales_order_line_type_code,
    quantity,
    unit_selling_price,
    local_currency_code,
    tax_entered_amt,
    freight_entered_amt,
    transaction_entered_amt,
    arrs_interface_line_context,
